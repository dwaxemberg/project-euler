/* Diego Waxemberg
 * Project Euler
 * Problem 1 
 */

public class Problem1 {
  
  public static main(String[] args) {
    int sum = 0;
    for (int i = 0; i < args[0]; i++) {
      if (i % 3 == 0 || i % 5 == 0)
        sum += i;
    }
    System.out.println("The sum of the numbers is: " + sum);
  }
}